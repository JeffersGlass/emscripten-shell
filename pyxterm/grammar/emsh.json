{
    "lex": {
        "rules": [
            [ "\\s+", "/*skip whitespace*/" ],
            [ "cat", "return 'CAT';"],
            [ "0-9+", "return 'NUMBER';"],
            [ "[a-fA-F0-9]+", "return 'HEX';" ]
            
        ]
    },
    "bnf": {
        "statement" :[
            "CAT value"
        ],
        "value": [
            "HEX",
            "NUMBER"
        ]
    }
}