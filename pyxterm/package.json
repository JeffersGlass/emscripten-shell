{
  "name": "py-xterm",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run build:grammar && rollup -c",
    "dev": "rollup -c -w",
    "build:grammar": "jison -j grammar/emsh.json -o grammar/build/emsh.js -m commonjs",
    "build:l": "jison -j grammar/emsh.l -o grammar/build/emsh.js -m commonjs"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "jison-gho": "^0.6.1-216",
    "rollup": "^3.10.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.4.1",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1"
  }
}
